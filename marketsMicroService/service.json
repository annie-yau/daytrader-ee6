{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "markets-microservice-jenkins",
    "namespace": "default",
    "selfLink": "/api/v1/namespaces/default/services/markets-microservice-jenkins",
    "labels": {
      "app": "markets-microservice-jenkins"
    }
  },
  "spec": {
    "ports": [
      {
        "name": "http",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 9086,
        "nodePort": 31421
      }
    ],
    "selector": {
      "app": "markets-microservice-jenkins"
    },
    "type": "NodePort",
    "sessionAffinity": "None"
  }
}
